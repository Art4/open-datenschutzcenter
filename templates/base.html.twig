<!DOCTYPE html>
<html>

{{ include('base/__head.html.twig') }}

<body>

<div id="mainContent" class="wrapper">
    {{ include('base/__sidenav.html.twig') }}

    <!-- Page Content  -->
    <div id="content">
        <nav class="navbar navbar-expand-lg navbar-light bg-light">
            <div class="container-fluid">

                <button type="button" id="sidebarCollapse" class="btn navbar-btn-navigation btn-sm rounded-pill">
                    Navigation
                </button>

                <!-- Button trigger modal -->
                <button class="btn navbar-btn-navigation btn-sm rounded-pill d-inline-block d-lg-none ml-auto"
                        type="button"
                        data-toggle="modal" data-target="#sideModalTR" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    {% trans %}Profil{% endtrans %}
                </button>

                {% block CTA %}{% endblock %}

                {{ include('base/__topnav.html.twig') }}
            </div>
        </nav>
        {% block body %}{% endblock %}

        {{ include('base/__footer.html.twig') }}
    </div>


    <div class="modal fade" id="loadContentModal" tabindex="-1" role="dialog" aria-labelledby="loadContentModal"
         aria-hidden="true">

    </div>
</div>
<!-- Dark Overlay element -->
<div class="overlay"></div>
{{ include('base/__script.html.twig') }}
{% block javascripts %}{% endblock %}
</body>

</html>
