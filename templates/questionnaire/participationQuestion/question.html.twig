{% extends 'base.html.twig' %}

{% block title %}
    {{ question.label }}
{% endblock %}

{% block body %}
    {{ form_start(form) }}
    <fieldset>
        {{ form_row( attribute(form, question.id)) }}
    </fieldset>
    {{ form_row(form.continue, {row_attr: {class: 'form-buttons'}}) }}
    {{ form_end(form) }}
{% endblock %}